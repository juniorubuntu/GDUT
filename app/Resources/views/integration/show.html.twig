{% extends 'base.html.twig' %}

{% block body %}
    <h1>Integration</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ integration.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ integration.code }}</td>
            </tr>
            <tr>
                <th>Libele</th>
                <td>{{ integration.libele }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('integration_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('integration_edit', { 'id': integration.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
